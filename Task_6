#include <stdio.h>

int main() {
    int a,b;
    printf("Enter the value of a: \n",&a);
    scanf("%d",&a);
    printf("Enter the value of b: \n",&b);
    scanf("%d",&b);
    if(a>b)
    {
        printf("a is largest");
    }
    else
    {
        printf("b is largest");
        
    }
    return 0;
}
