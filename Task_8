#include <stdio.h>

int main() {
    int a,b,temp;
    printf("Ener the value of a :\n",&a);
    scanf("%d",&a);
    printf("Enter the value of b :\n",&b);
    scanf("%d",&b);
    printf("Before swapping a :%d b :%d\n",a,b);
    temp=a;
    a=b;
    b=temp;
    printf("After swapping a :%d b :%d \n",a,b);
    return 0;
}
