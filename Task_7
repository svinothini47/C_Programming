#include <stdio.h>

int main() {
    int a,b,c;
    printf("Enter the value of a: \n",&a);
    scanf("%d",&a);
    printf("Enter the value of b: \n",&b);
    scanf("%d",&b);
    printf("Enter the value of c: \n",&c);
    scanf("%d",&c);
    if((a>b) && (a>c))
    {
        printf("a is highest");
    }
    else if ((b>a) && (b>c))
    {
        printf("b is highest");
    }
    else
    {
        printf("c is largest");
        
    }
    return 0;
}
