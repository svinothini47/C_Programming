#include <stdio.h>

int main() {
    int a,b;
    printf("Enter a value :\n",&a);
    scanf("%d",&a);
    printf("Enter b value :\n",&b);
    scanf("%d",&b);
    printf("Before swapping a :%d , b :%d\n",a,b);
    a=a+b;
    b=a-b;
    a=a-b;
    printf("After swapping a :%d , b : %d\n",a,b);
    return 0;
}
