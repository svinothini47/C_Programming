#include <stdio.h>

int main() {
    int a,b;
    printf("Enter value of a :\n",&a);
    scanf("%d",&a);
    printf("Enter value of b :\n",&b);
    scanf("%d",&b);
    printf("Sum :%d\n",a+b);
    printf("Difference :%d\n",a-b);
    printf("Product :%d\n",a*b);
    printf("Quotient :%d\n",a/b);
    return 0;
}
